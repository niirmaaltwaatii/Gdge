{
   "description": "Gdge Theme for Chromium based Web Browsers !",
   "icons": {
      "128": "icon.png"
   },
   "manifest_version": 2,
   "name": "Gdge",
   "theme": {
      "clear_new_tab": {
         "size": "cover_screen"
      },
      "colors": {
         "background_tab": [0,13,77],//inac
         "background_tab_inactive": [100,50,100],//bginac
         "bookmark_text": [255,255,255],
         "frame": [0,130,127],
         "frame_inactive": [45,45,45],
         "ntp_background": [0,0,0],
         "ntp_link": [0,0,128 ],
         "ntp_text": [255,255,255],
         "omnibox_background": [100,50,100],//urlbr
         "omnibox_text": [255,255,255],
         "tab_background_text": [255,255,255],
         "tab_background_text_inactive": [224,224,224],
         "tab_text": [255,255,255],
         "toolbar": [0,128,20],//mstr
         "toolbar_button_icon": [255,255,255]
      },
      "images": {
         "theme_ntp_background": "images/bgwpp.jpg"
      },
      "properties": {
         "ntp_background_alignment": "center",
         "ntp_background_repeat": "no-repeat",
         "ntp_logo_alternate": 1
      },
      "tints": {
         "background_tab": [ -1, -1, -1 ],
         "buttons": [ -1, -1, 1 ],
         "frame": [ -1, -1, -1 ]
      }
   },
   "update_url": "https://github.com/niirmaaltwaatii/Gdge",
   "version": "1.0.0"
}
